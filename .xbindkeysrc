# Increase Volume
"amixer set Master 5%+"
    m:0x0 + c:123

# Decrease Volume
"amixer set Master 5%-"
    m:0x0 + c:122
# Mute Volume
"amixer set Master toggle"
    m:0x0 + c:121

# Brighter
"brightnessctl -d intel_backlight set +20%"
    XF86MonBrightnessUp

# Dimmer
"brightnessctl -d intel_backlight set 20%-"
    XF86MonBrightnessDown

# Play/Pause
"playerctl play-pause"
    m:0x0 + c:172

# Next Media
"playerctl next"
    m:0x0 + c:171

# Prev Media
"playerctl previous"
    m:0x0 + c:173

# Lock Screen
"xscreensaver-command -lock"
    XF86AudioMedia
